t_compile_module($init, <<'END', $objs);
	int yy_input( char *buffer, size_t size );
	FILE *fp;
	int read, i, j;
	char buffer[16];
	
	for ( i = 1 ; 1 ; i++ )
	{
		sprintf( buffer, "test%d.asm", i );
		fp = fopen( buffer, "rb" );
		if ( fp == NULL )
			return 0;
		fclose( fp );
			
		warn("Read file %s:\n", buffer );
		scan_file( buffer );
		
		while ( (read = yy_input( buffer, sizeof(buffer))) > 0 )
		{
			for ( j = 0; j < read ; j++ )
			{
				if ( buffer[j] > ' ' )
					warn("%c", buffer[j] );
				else
					warn("<%02X>", buffer[j] );
			}
			warn("\n");
		}
		n_tokens( -1 );
		
	}
	return 0;

END

